{% extends 'acceilfarm.html.twig' %}

{% block title %}Produit{% endblock %}

{% block body %}
    <h1>Produit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdProd</th>
                <td>{{ produit.idProd }}</td>
            </tr>
            <tr>
                <th>NomProd</th>
                <td>{{ produit.nomProd }}</td>
            </tr>
            <tr>
                <th>Discription</th>
                <td>{{ produit.discription }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ produit.quantite }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ produit.prix }}</td>
            </tr>
            <tr>
                <th>Categories</th>
                <td>{{ produit.categories }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('produit/_delete_form.html.twig') }}
{% endblock %}
